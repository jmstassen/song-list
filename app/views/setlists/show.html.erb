<%= form_for(@setlist) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <br>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <br>
    <br>
    <%= f.label :date %>
    <%= f.date_field :date %>
    <br>
    <br>
    <%= f.submit "Create" %>
  <% end %>

  <% @setlist.songs.each do |s| %>
    <%= link_to s.title, setlist_path(s) %> | <%= s.id %>
    <br>
  <% end %>