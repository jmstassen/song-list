<br>
<h1>Welcome <%= @user.display_name %></h1>
<%= link_to "Settings", edit_user_path(@user) %>
<br>
<br>
<h2>Your Setlists</h2>
<% @setlists.each do |s| %>
    <%= link_to s.title, setlist_path(s) %> | <%= s.id %>
    <br>
  <% end %>
<br>
<h2>Your Songs</h2>
<% @songs.each do |s| %>
  <%= link_to s.title, song_path(s) %> | <%= s.id %>
  <br>
<% end %>