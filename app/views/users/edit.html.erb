<br>
<h1>Settings</h1>
<br>
<%= render partial: "layouts/errors", locals: {object: @user} %>

<%= form_for(@user) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>
  <br>
  <%= f.label :display_name %>
  <%= f.text_field :display_name %>
  <br>
  <br>
  <%= f.submit "Update" %>

<% end %>