<h1>New Song</h1>

<%= form_for(@song) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <br>
  <%= f.label :songwriter %>
  <%= f.text_field :songwriter %>
  <br>
  <br>
  <%= f.label :tempo %>
  <%= f.text_field :tempo %>
  <br>
  <br>
  <%= f.label :key %>
  <%= f.text_field :key %>
  <br>
  <br>
  <%= f.label :note %>
  <%= f.text_area :note %>
  <br>
  <br>
  <%= f.label :link %>
  <%= f.text_field :link %>
  <br>
  <br>
  <%= f.submit "Create" %>
<% end %>